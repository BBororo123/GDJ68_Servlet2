package com.bbo.study4.s3;

public class Car {
	
	String company;
	String brand;
	String color;
	int wheel;
	
	public void info() {
		System.out.println("Company : "+this.company);
		System.out.println("Brand : "+this.brand);
		System.out.println("Color : "+this.color);
		System.out.println("Wheel : "+this.wheel);
	}

}
